{
    "name": "wild-breeze-api",
    "version": "1.0.0",
    "description": "Cloudflare Workers API for Wild-Breeze",
    "main": "src/index.js",
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy",
        "db:create": "wrangler d1 create wild-breeze-db",
        "db:init": "wrangler d1 execute wild-breeze-db --file=./schema.sql",
        "db:init:local": "wrangler d1 execute wild-breeze-db --local --file=./schema.sql"
    },
    "devDependencies": {
        "wrangler": "^4.63.0"
    }
}
